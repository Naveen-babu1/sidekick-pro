{
  "name": "sidekick-pro",
  "displayName": "Sidekick Pro",
  "description": "AI-powered development assistant",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.74.0"
  },
  "main": "./out/main.js",
  "activationEvents": [],
  "contributes": {
    "commands": [
      {
        "command": "sidekick.chat.open",
        "title": "Sidekick Pro: Open Chat"
      },
      {
        "command": "sidekick.search",
        "title": "Sidekick Pro: Search Repository"
      },
      {
        "command": "sidekick.dashboard.open",
        "title": "Sidekick Pro: Open Dashboard"
      },
      {
        "command": "sidekick.index",
        "title": "Sidekick Pro: Index Repository"
      },
      {
        "command": "sidekick.switchModel",
        "title": "Sidekick Pro: Switch Model Provider"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.0"
  },
  "dependencies": {
    "dotenv": "^16.0.3"
  }
}
